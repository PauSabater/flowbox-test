---
import '@fontsource-variable/inter'

interface Props {
	title: string;
}

// const imagesToCache: string[] = getImagesToCache()

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Technical Task for Front-end Developer at Flowbox, made by Pau Sabater" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>
